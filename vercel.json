{
  "functions": {
    "api/subtitles.js": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "YOUTUBE_API_KEY": "@youtube_api_key"
  }
}
